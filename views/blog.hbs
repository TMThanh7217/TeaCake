<div class="container">
    {{#each rows}}    
    <div class="row card-group">
        {{#each this}}
        <div class="col-sm-8 col-md-6 col-lg-4 g-mb-30 tb-cell blog-box">
            <div class="card u-shadow-v18 g-bg-white text-center rounded g-px-20 g-py-40 g-mb-5 blog-card h-100">
                <div class="card-header d-flex align-items-center justify-content-center g-bg-white">
                    <img src="{{this.img}}" alt="" class="img-thumbnail product-img" onclick='document.location.href="/blog?id={{this.id}}"'>
                </div>
                <div class="card-body d-flex flex-column">
                    <h4 class="h5 g-color-black g-font-weight-600 g-mb-10 blog-header">{{this.header}}</h4>
                    <p class="blog-content">{{this.content}}</p>
                    <span class="blog-author">{{this.author}}</span><span class="blog-postDate">{{this.postDate}}</span>    
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    {{/each}}
</div>